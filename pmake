#!/usr/bin/perl
# Maximillian Nibler mnibler@ucsc.edu

use strict;
use warnings;

printf "Makefile \n";

open(DATA, "<Makefile") or die "Couldn't open file Makefile, $!";

while(<DATA>) {
    my $parse = "$_";
    my $line = $parse =~ m/#/? "$`" : "$parse";
    puthash ($line);
}
# just testing git
my %lineHash = ();
sub puthash {
    $lineHash{"$@_"} = "@_";
    print $lineHash{"$@_"};
}

